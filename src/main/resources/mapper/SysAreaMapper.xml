<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiadun.dispatch.mapper.SysAreaMapper">

    <select id="getAreaTree" resultType="com.jiadun.dispatch.vo.sys.res.SysAreaTreeVo">
        select a.id,a.code,a.p_code,a.name,a.type from t_sys_area a
        where a.code like '53%';
    </select>
</mapper>
